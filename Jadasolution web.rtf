\
    <!-- JavaScript Logic -->\
    <script>\
        // --- State Management ---\
        const state = \{\
            currentSection: 'section-executive',\
            problemChartData: \{\
                traditional: [100, 15, 15, 15, 100], // Dog-bone: Thick ends, thin middle\
                jada: [25, 24, 25, 24, 25]           // Uniform fill\
            \}\
        \};\
\
        // --- Navigation Logic ---\
        function navigateTo(sectionId) \{\
            // Hide all sections\
            document.querySelectorAll('.page-section').forEach(el => \{\
                el.classList.add('hidden');\
            \});\
            \
            // Show target section\
            const target = document.getElementById(sectionId);\
            target.classList.remove('hidden');\
            \
            // Update Nav State (Desktop)\
            document.querySelectorAll('.nav-item').forEach(el => \{\
                el.classList.remove('bg-brand-bg', 'text-brand-accent', 'border-brand-accent');\
                el.classList.add('text-brand-secondary', 'border-transparent');\
                \
                // If this is the button clicked (simple match)\
                if (el.getAttribute('onclick').includes(sectionId)) \{\
                    el.classList.add('bg-brand-bg', 'text-brand-accent', 'border-brand-accent');\
                    el.classList.remove('text-brand-secondary', 'border-transparent');\
                \}\
            \});\
\
            // Scroll to top\
            document.querySelector('main').scrollTop = 0;\
            \
            // Render Charts if entering section\
            if(sectionId === 'section-problem' && window.problemChartInstance) \{\
                // Ensure chart redraws correctly if size changed\
                window.problemChartInstance.resize();\
            \}\
        \}\
\
        // Mobile Menu Toggle\
        document.getElementById('mobile-menu-btn').addEventListener('click', () => \{\
            const sidebar = document.getElementById('sidebar');\
            sidebar.classList.toggle('hidden');\
            sidebar.classList.toggle('absolute'); // Make it overlay on mobile\
            sidebar.classList.toggle('top-14'); // Adjust for header\
            sidebar.classList.toggle('z-40');\
        \});\
\
        // --- Chart Configurations ---\
\
        // 1. Problem Chart (Bar Chart simulating Hole Thickness)\
        const ctxProblem = document.getElementById('problemChart').getContext('2d');\
        window.problemChartInstance = new Chart(ctxProblem, \{\
            type: 'bar',\
            data: \{\
                labels: ['\uc0\u23380 \u21475 (Top)', '\u19978 \u37096 (Upper)', '\u20013 \u37096 (Middle)', '\u19979 \u37096 (Lower)', '\u23380 \u24213 (Bottom)'],\
                datasets: [\{\
                    label: '\uc0\u38108 \u23618 \u21402 \u24230  (\u956 m)',\
                    data: state.problemChartData.traditional, // Start with bad data\
                    backgroundColor: 'rgba(239, 68, 68, 0.7)', // Red for bad\
                    borderColor: 'rgba(239, 68, 68, 1)',\
                    borderWidth: 1\
                \}]\
            \},\
            options: \{\
                responsive: true,\
                maintainAspectRatio: false,\
                scales: \{\
                    y: \{\
                        beginAtZero: true,\
                        max: 120,\
                        title: \{ display: true, text: '\uc0\u21402 \u24230  (\u956 m)' \}\
                    \}\
                \},\
                plugins: \{\
                    legend: \{ display: false \},\
                    tooltip: \{\
                        callbacks: \{\
                            label: function(context) \{\
                                return context.raw + ' \uc0\u956 m';\
                            \}\
                        \}\
                    \}\
                \}\
            \}\
        \});\
\
        function updateProblemChart(mode) \{\
            const chart = window.problemChartInstance;\
            const caption = document.getElementById('chart-caption');\
            const btnTrad = document.getElementById('btn-trad');\
            const btnJada = document.getElementById('btn-jada');\
\
            if (mode === 'traditional') \{\
                chart.data.datasets[0].data = state.problemChartData.traditional;\
                chart.data.datasets[0].backgroundColor = 'rgba(239, 68, 68, 0.7)';\
                chart.data.datasets[0].borderColor = 'rgba(239, 68, 68, 1)';\
                caption.innerHTML = "\uc0\u20256 \u32479 \u24037 \u33402 \u65306 \u26126 \u26174 \u30340 <span class='text-red-600 font-bold'>\'93\u29399 \u39592 \u25928 \u24212 \'94</span>\u65292 \u23380 \u21475 \u21402 (100\u956 m)\u65292 \u23380 \u20869 \u34180 (15\u956 m)\u12290 ";\
                \
                btnTrad.classList.add('ring-2', 'bg-red-50');\
                btnJada.classList.remove('ring-2', 'bg-green-50');\
            \} else \{\
                chart.data.datasets[0].data = state.problemChartData.jada;\
                chart.data.datasets[0].backgroundColor = 'rgba(34, 197, 94, 0.7)';\
                chart.data.datasets[0].borderColor = 'rgba(34, 197, 94, 1)';\
                caption.innerHTML = "Jada PVCP\uc0\u65306 <span class='text-green-600 font-bold'>\u22343 \u21248 \u19968 \u33268 </span>\u65292 \u21508 \u20301 \u32622 \u21402 \u24230 \u25509 \u36817  25\u956 m (COV < 5%)\u12290 ";\
\
                btnJada.classList.add('ring-2', 'bg-green-50');\
                btnTrad.classList.remove('ring-2', 'bg-red-50');\
            \}\
            chart.update();\
        \}\
\
        // 2. Market Growth Chart (TAM)\
        const ctxMarket = document.getElementById('marketChart').getContext('2d');\
        new Chart(ctxMarket, \{\
            type: 'line',\
            data: \{\
                labels: ['2022', '2023', '2024', '2025(E)', '2026(E)'],\
                datasets: [\{\
                    label: '\uc0\u24066 \u22330 \u35268 \u27169  ($ Billion)',\
                    data: [1.2, 1.4, 1.75, 2.2, 2.8],\
                    borderColor: '#0EA5E9', // brand-accent\
                    backgroundColor: 'rgba(14, 165, 233, 0.1)',\
                    fill: true,\
                    tension: 0.4\
                \}]\
            \},\
            options: \{\
                responsive: true,\
                maintainAspectRatio: false,\
                plugins: \{\
                    legend: \{ display: false \}\
                \}\
            \}\
        \});\
\
        // 3. Value Multiplier (Donut)\
        const ctxValue = document.getElementById('valueMultiplierChart').getContext('2d');\
        new Chart(ctxValue, \{\
            type: 'doughnut',\
            data: \{\
                labels: ['\uc0\u26222 \u36890 \u26381 \u21153 \u22120  PCB', 'AI \u26381 \u21153 \u22120  PCB (\u22686 \u37327 )'],\
                datasets: [\{\
                    data: [1, 5],\
                    backgroundColor: ['#E5E7EB', '#D97706'], // Grey vs Amber\
                    hoverOffset: 4\
                \}]\
            \},\
            options: \{\
                responsive: true,\
                maintainAspectRatio: false,\
                cutout: '70%',\
                plugins: \{\
                    legend: \{ position: 'bottom', labels: \{ boxWidth: 10 \} \}\
                \}\
            \}\
        \});\
\
        // 4. Economics Chart (Stacked Bar)\
        const ctxEcon = document.getElementById('economicsChart').getContext('2d');\
        new Chart(ctxEcon, \{\
            type: 'bar',\
            data: \{\
                labels: ['\uc0\u36816 \u33829 \u25104 \u26412  (\u19975 \u20803 /\u24180 )'],\
                datasets: [\
                    \{\
                        label: 'Jada VCP',\
                        data: [100], // Baseline representation\
                        backgroundColor: '#0F172A',\
                    \},\
                    \{\
                        label: '\uc0\u33410 \u30465 \u25104 \u26412  (Savings)',\
                        data: [130],\
                        backgroundColor: '#22C55E', // Green\
                    \}\
                ]\
            \},\
            options: \{\
                indexAxis: 'y',\
                responsive: true,\
                maintainAspectRatio: false,\
                scales: \{\
                    x: \{ stacked: true \},\
                    y: \{ stacked: true \}\
                \},\
                plugins: \{\
                    tooltip: \{\
                        callbacks: \{\
                            footer: () => '\uc0\u23545 \u27604 \u20256 \u32479 \u40857 \u38376 \u32447 \u65292 Jada \u27599 \u24180 \u33410 \u30465 \u32422  130 \u19975 \u20803 \u12290 '\
                        \}\
                    \}\
                \}\
            \}\
        \});\
\
        // 5. Radar Chart (Competition)\
        const ctxRadar = document.getElementById('radarChart').getContext('2d');\
        new Chart(ctxRadar, \{\
            type: 'radar',\
            data: \{\
                labels: ['\uc0\u28145 \u23380 \u33021 \u21147 (AR)', '\u27905 \u20928 \u24230 ', '\u26448 \u26009 \u21327 \u21516 ', '\u23450 \u21046 \u21709 \u24212 ', '\u36816 \u33829 \u25104 \u26412 '],\
                datasets: [\{\
                    label: 'Jada Solution',\
                    data: [95, 90, 100, 95, 90],\
                    fill: true,\
                    backgroundColor: 'rgba(14, 165, 233, 0.2)',\
                    borderColor: '#0EA5E9',\
                    pointBackgroundColor: '#0EA5E9',\
                \}, \{\
                    label: '\uc0\u20256 \u32479 \u36890 \u29992 \u35774 \u22791 \u21830 ',\
                    data: [60, 50, 40, 60, 60],\
                    fill: true,\
                    backgroundColor: 'rgba(156, 163, 175, 0.2)',\
                    borderColor: '#9CA3AF',\
                    pointBackgroundColor: '#9CA3AF',\
                \}]\
            \},\
            options: \{\
                responsive: true,\
                maintainAspectRatio: false,\
                scales: \{\
                    r: \{\
                        angleLines: \{ display: false \},\
                        suggestedMin: 0,\
                        suggestedMax: 100\
                    \}\
                \}\
            \}\
        \});\
\
    </script>\
</body>\
</html>}